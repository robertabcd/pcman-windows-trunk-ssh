<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
	<ShortName>教育部國語辭典</ShortName>
	<Description>教育部國語辭典</Description>
	<InputEncoding>big5</InputEncoding>
	<Image width="16" height="16">data:image/gif;base64,</Image>
	<Url type="application/x-suggestions+json" method="GET" template="http://suggestqueries.google.com/complete/search?output=firefox&amp;client=firefox&amp;qu={searchTerms}"/>
	<Url type="text/html" method="GET" template="http://dict.revised.moe.edu.tw/cgi-bin/newDict/dict.sh">
		<Param name="idx" value="dict.idx"/>
		<Param name="syn" value="1"/>
		<Param name="pieceLen" value="50"/>
		<Param name="fld" value="1"/>
		<Param name="cat" value=""/>
		<Param name="imgFont" value="1"/>
		<Param name="cond" value="{searchTerms}"/>
	</Url>
	<SearchForm>http://dict.revised.moe.edu.tw/index.html</SearchForm>
</SearchPlugin>